package strings;

import java.util.Scanner;

public class StringsIntroduction {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		String A = sc.next();
		String B = sc.next();
		System.out.println(A.length() + B.length());
		System.out.println(A.compareTo(B));
		sc.close();
	}
}
